<script src="~/Scripts/External/nml/form-submit-validate.js"></script>
<script type="text/javascript">
    docReady(function () {

        jQuery(".button--primary").on("click", function () {

            setTimeout(function () {
                var id = GetErrorMessageId();
                if (id != "") {
                    //var _url = window.location.href;
                    //window.location.href = "#" + id;
                    $('html, body').animate({ scrollTop: $('#' + id).offset().top - 200 }, 'slow');
                    history.pushState({}, '', _url);
                };
            }, 100);
        });
        function GetErrorMessageId() {
            var _Id = "";
            $.each($(".parsley-errors-list"), function (i, obj) {
                if (_Id == "" && $(obj).find("li").length > 0) {
                    if (_Id == "") {
                        _Id = jQuery(obj).prop("id");
                    }
                }
            })
            return _Id;
        }

        // Step tracker function
        var total = parseInt(5);
        var current = parseInt(1);
        var txt = jQuery("#divstep_1").find('.legend').text();
        steptracker(total, current, txt);
        var $sections = jQuery('.sponsorshipsection');
        function steptracker(total, current, txt) {
            $('.m38-step-tracker').attr('data-total-steps', total);
            $('.m38-step-tracker').attr('data-current-step', current);
            $('.m38-step-tracker__progressbar').attr('aria-valuetext', txt);
            $('.m38-step-tracker').attr('data-hasInfo', "true");
            jQuery(window).trigger('reinit_m38');
        }
        function curIndex() {
            // Return the current index by looking at which section has the class 'current'
            return $sections.index($sections.filter('.current'));
        }
        function navigateTo(index) {
            var txt = $sections.hide().eq(index).find('.legend').text();
            steptracker(total, index + 1, txt);

            // Mark the current section with the class 'current'
            $sections
                .removeClass('current')
                .eq(index)
                .addClass('current');
            $sections
                .hide()
                .eq(index)
                .show();


            // Show only the navigation buttons that make sense for the current section:
            jQuery('.form-navigation .previous').toggle(index > 0);
            var atTheEnd = index >= $sections.length - 1;
            jQuery('.form-navigation .next').toggle(!atTheEnd);
            jQuery('.form-navigation [type=submit]').toggle(atTheEnd);
            jQuery('html,body').animate({ scrollTop: 0 }, 1000);
            if (atTheEnd) {
            }
        }

        // Next button click event
        jQuery('.form-navigation .next').click(function (e) {
            jQuery('.m26-page-title__backlink').hide();
            $('.form-field--text_add-goal input').val('');
            $('.form-field--text_add-goal input').removeClass('form-field__input--error');
            $('.form-field--text_add-goal input').removeClass('parsley-error');
            $('.form-field--text_add-goal .form-field--add_wrapper').removeClass('form-field__input-wrapper--error');
            require(['parsley'], function () {
                if (jQuery('#IdSponsorshipdetails').parsley().validate()) {
                    e.preventDefault();
                    navigateTo(curIndex() + 1);
                };
            });

        });

        // Previous button is easy, just go back
        jQuery('.form-navigation .previous').click(function (e) {
            e.preventDefault();
            navigateTo(curIndex() - 1);
            if (curIndex() == 0) {
                jQuery('.m26-page-title__backlink').show();
            }
        });


        require(['parsley'], function () {
            // submit button click event
            jQuery('.button--submit').off('click.submit').on('click.submit', function (e) {
                setEventgoal();
                setInvitedVIPs();
                setBeneficiaries();
                try {
                    if (!jQuery('#IdSponsorshipdetails').parsley().validate() || !recaptchaCallback()) {
                        e.preventDefault();
                        return false;
                    }
                    else {
                        jQuery('.j105-drrg--loader').show();
                        jQuery('.j105-drrg--loader').css('top', $(window).scrollTop());
                        $('body').removeClass('unscrollable').addClass('unscrollable');
                        return true;
                    }
                } catch (e) {
                    console.log(e);
                    jQuery('.j105-drrg--loader').hide();
                    $('body').removeClass('unscrollable');
                }
                return false;
            });
        });

        setTimeout(function () { navigateTo(0); }, 200);

        //recaptcha callback function
        function recaptchaCallback() {
            if (jQuery("#captchaac_form").length > 0) {
                var v = grecaptcha.getResponse();
                if (v.length == 0) {
                    jQuery('#errorContainer').text('@Translate.Text("You cant leave Captcha Code empty")');
                    return false;
                }
                else {
                    jQuery('#errorContainer').text("");
                    return true;
                }
            }
            return true;
        }

        //Event of the goals
        function setEventgoal() {
            var goals = [];
            var addTableWrapper = jQuery('.form-field--table-goals'),
                addTableRow = addTableWrapper.find('.form-field--table-row');

            if (addTableRow.length > 0) {
                addTableRow.each(function () {
                    var items = jQuery(this).find('.form-field--table-list-item');

                    items.each(function () {
                        var val = $(this).text();
                        goals.push(val);
                    })
                })
            }
            $("#ideventgoals").val(goals.join(","));
        }

        //Invited VIPs
        function setInvitedVIPs() {
            var invitedVIP = [];
            var addTableWrapper1 = jQuery('.form-field--table-eventinvitedvip'),
                addTableRow1 = addTableWrapper1.find('.form-field--table-row');

            if (addTableRow1.length > 0) {
                addTableRow1.each(function () {
                    var items1 = jQuery(this).find('.form-field--table-list-item');

                    items1.each(function () {
                        var val1 = $(this).text();
                        invitedVIP.push(val1);
                    })
                })
            }
            $("#ideventinvitedvip").val(invitedVIP.join(","));
        }

        // Beneficiaries
        function setBeneficiaries() {
            var beneficiaries = [];
            var addTableWrapper2 = jQuery('.form-field--table-eventbeneficiaries'),
                addTableRow2 = addTableWrapper2.find('.form-field--table-row');

            if (addTableRow2.length > 0) {
                addTableRow2.each(function () {
                    var items2 = jQuery(this).find('.form-field--table-list-item');

                    items2.each(function () {
                        var val2 = $(this).text();
                        beneficiaries.push(val2);
                    })
                })
            }
            $("#ideventbeneficiaries").val(beneficiaries.join(","));
        }
    });

</script>
