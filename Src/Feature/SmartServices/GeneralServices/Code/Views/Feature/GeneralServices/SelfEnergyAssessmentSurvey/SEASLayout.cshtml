@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Mvc.Configuration
@model RenderingModel
<div id="rs_area">
    <div class="rs_preserve">
        @Html.Sitecore().Placeholder("main/header")
        <div class="j69-dashboard2">

            <div class="grid">
                <div class="grid__row mb24 grid__row-tight_gutter">
                    <div class="grid__column grid__column--6">
                        <br />
                        @using (Html.BeginRouteForm(MvcSettings.SitecoreRouteName, FormMethod.Post, new { @id = "form-account-selector", @class = "form form-dynamic", data_form = "true", encType = "multipart/form-data", @data_submit_validate = "enabled" }))
                        {
                                @Html.AntiForgeryToken()
                                <fieldset class="fieldset ">
                                    @Html.Sitecore().Placeholder("accountselector")
                                </fieldset>
                        }

                    </div>
                    <div class="grid__column grid__column--6">

                    </div>
                </div>
            </div>
            @Html.Sitecore().Placeholder("main/content")
        </div>
        @Html.Sitecore().Placeholder("main/bottom-section")

        @Html.Sitecore().Placeholder("main/rammas")
        @Html.Sitecore().Placeholder("main/cp-content")
    </div>
</div>

@Html.Sitecore().Placeholder("main/footer")


