                beforeSend: function () {
                    window.attachSpinner('#move-out-details', { zIndex: 1, bgColor: "#fff", bgPosition: "top center" });
                    $('#western_union_selectstate-' + index).hide();
                    $('#western_union_selectcity-' + index).hide();
