<script type="text/javascript">
    $(document).ready(function () {
        $("#hd_AccSelectection").val("");
        $(".m43-accountsel-hidden .m39-modal__footer .button--primary").on("click", onAccountSelection);
        onAccountSelection();
    });

    function handleAccountNumber() {
        if ($("#form-field-radio_1_1").prop("checked")) {
            $("#hd_AccSelectection").val(1);
            if ($('#ContractAccountfile').val() == '') {
                alert("@Translate.Text("JO.AccountFileMissing")");
                return false
            }
        }
        else if ($("#form-field-radio_1_2").prop("checked")) {
            $("#hd_AccSelectection").val(2);

            var list = $("#premiseAccountList li").map(function () {
                return this.textContent.trim();
            }).get();

            if (list.length <= 0 && ($("#form-field-premiseNumber").val() == "" || $("#form-field-premiseNumber").val() == null)) {
                alert("@Translate.Text("JO.OneAccountNeeded")");
                return false
            }

            var array = [];

            if (list.length > 0) {
                $.each(list, function (idx2, val2) {
                    var str = val2.split(' ');
                    array.push(str[0]);
                });
            }
            else {
                array.push($("#form-field-premiseNumber").val());
            }

            var acc = array + "";

            $("#hd_AccountList").val(acc);

            return true;
        }
    }

    function submitForm() {
        if (handleAccountNumber()) {
            if ($('#form-joint-owner').parsley().validate()) {
                $('#form-joint-owner').submit();
            }
        }
    }

    function onAccountSelection() {
        var SelectedAccountElement = $("input[name='SelectedAccountNumber']:checked");
        $("#Contractaccount").val($(SelectedAccountElement).val());
        $(".m37-expander").hide();
    }

</script>

