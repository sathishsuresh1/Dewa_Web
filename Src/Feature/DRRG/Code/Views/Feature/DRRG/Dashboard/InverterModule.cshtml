<script type="text/javascript">
    docReady(function () {
        var totalstep = 3;
        var current = 1;
        var txt = jQuery('.pageclass[data-page=0]').find('.legend').text();
        steptracker(current, txt);
        var modelName = jQuery("#form-field-ModelName");
        var errModelname = jQuery("#description-for-ModelName");
        var $submitBtn = jQuery("#btSubmit");
        $('#btSubmit').click(function (e) {
            e.preventDefault();
            if (jQuery(".j103-drrg-signature").attr("sig-data") != '' && jQuery(".j103-drrg-signature").attr("sig-data") != undefined) {
                jQuery("#signatureCopy").val(jQuery(".j103-drrg-signature").attr("sig-data"));
                jQuery("#signature-error-message").hide();
                jQuery("#btSubmit").html("@Translate.Text("Submitting")...");
                jQuery("#btSubmit").attr("disabled", true);
                $('#drrginverterform').submit();
                return true;
            }
            else {
                jQuery("#signature-error-message").show();
                jQuery("#btSubmit").attr("disabled", false);
                jQuery("#btSubmit").html("@Translate.Text("DRRG_SignSubmit")");
                return false;
            }
        });
        function steptracker(current, txt) {
            $('.m38-step-tracker').attr('data-total-steps', totalstep);
            $('.m38-step-tracker').attr('data-current-step', current);
            $('.m38-step-tracker__progressbar').attr('aria-valuetext', txt);
            $('.m38-step-tracker').attr('data-hasInfo', "true");
            jQuery(window).trigger('reinit_m38');
        }
        var $groups0 = $('[data-uploadgroup-logic]');
        $groups0.each(function () {
            var $parent = $(this);
            $(this).find('input[type="file"]').each(function () {
                $(this).on('change', function () {
                    $parent.removeClass('skiplogic');

                });
            });
            $(this).find('[data-uploader-remove]').on('click', function () {
                $parent.removeClass('skiplogic');
            });
        });

        $("#form-field-DewaDRRGStandard").on('change', function () {
            $(this).prop('required', true);
        });
        $("#form-field-DewaDRRGStandard").closest('.form-field').find('[data-uploader-remove]').on('click', function () {
            $("#form-field-DewaDRRGStandard").prop('required', true);
        });
        $("#form-field-HarmonicSpectrum").on('change', function () {
            $(this).prop('required', true);
        });
        $("#form-field-HarmonicSpectrum").closest('.form-field').find('[data-uploader-remove]').on('click', function () {
            $("#form-field-HarmonicSpectrum").prop('required', true);
        });

        jQuery('.nextbutton').click(function (e) {
            e.preventDefault();

            var pagenumber = parseInt(jQuery(this).closest('.pageclass').attr("data-page"));
            if (pagenumber == 1) {
                //$form.off('click.j03').on('click.j03', function (event) {
                // if (string.IsNullOrWhiteSpace(Model.InverterID) && (Model.Status == null || (Model.Status != null && !Model.Status.Equals("Submitted"))))
                var invId = '@Model.InverterID';
                var status = '@Model.Status';
                if (status != 'Approved') {
                    jQuery("#form-field-DewaDRRGStandard").parsley().validate();
                    jQuery("#form-field-HarmonicSpectrum").parsley().validate();
                }

                if ((invId == '' && status == '') || (invId != '' && status == 'ReviewerRejected')) {
                    var $groups = $('[data-uploadgroup-logic]'),
                        errorcount = 0;
                    $groups.each(function () {

                        if (!$(this).hasClass('skiplogic')) {
                            var logic = $(this).attr('data-uploadgroup-logic'),
                                $grouperror = $(this).find('.form-field--upload_grouperror');
                            if (!Function('"use strict";return (' + logic + ')')()) {
                                errorcount++;
                                $grouperror.show();
                            } else {
                                $grouperror.hide();
                            };
                        } else {
                            $(this).find('.form-field--upload_grouperror').hide();
                        }
                    });
                    if (errorcount != 0) {
                        e.preventDefault();
                        $(window).scrollTop($('.form-field--upload_grouperror[style="display: block;"]').offset().top - 150);
                    } else {
                        if (status != 'Approved')
                        {
                            if (jQuery("#form-field-DewaDRRGStandard").parsley().isValid() && jQuery("#form-field-HarmonicSpectrum").parsley().isValid()) {
                                nexpageload(pagenumber);
                            }
                        }
                        else
                        {
                            nexpageload(pagenumber);
                        }
                    }
                }
                else {
                    if (status != 'Approved') {
                        if (jQuery("#form-field-DewaDRRGStandard").parsley().isValid() && jQuery("#form-field-HarmonicSpectrum").parsley().isValid()) {
                            nexpageload(pagenumber);
                        }
                    }
                    else {
                        nexpageload(pagenumber);
                    }
                }

            } else {
                require(['parsley'], function () {
                    jQuery('#drrginverterform').parsley().validate();
                    if (jQuery('#drrginverterform').parsley().isValid()) {
                        nexpageload(pagenumber);
                    };
                });
            }
        });
        function nexpageload(pagenumber) {
            jQuery('.pageclass[data-page=' + (pagenumber) + ']').addClass('hidden');
            jQuery('.pageclass[data-page=' + (pagenumber + 1) + ']').removeClass('hidden');
            jQuery('.pageclass').not(jQuery('.pageclass[data-page=' + (pagenumber + 1) + ']')).addClass('hidden');
            var txt = jQuery('.pageclass[data-page=' + (pagenumber + 1) + ']').find('.legend').text();
            steptracker(pagenumber + 2, txt);
            if (pagenumber == 1) {
                jQuery(window).trigger('resize');
                var ratedPower = '';
                var maximumAcApparentPower = '';
                var maximumActivePower = '';
                jQuery('.modelname').html(HtmlSanitizer.SanitizeHtml(jQuery('#form-field-ModelName').val()));
                $("input[name^='RatedPower']").each(function () {
                    if (ratedPower != '')
                        ratedPower += $(this).val() + ' @Translate.Text("kW"), ';
                    else
                        ratedPower = $(this).val() + ' @Translate.Text("kW"), ';
                });
                jQuery('.ratedpower').html(ratedPower.replace(/,(\s+)?$/, ''));
                $("input[name^='MaximumAcApparentPower']").each(function () {
                    if (maximumAcApparentPower != '')
                        maximumAcApparentPower += $(this).val() + ' @Translate.Text("kVA"), ';
                    else
                        maximumAcApparentPower = $(this).val() + ' @Translate.Text("kVA"), ';
                });
                jQuery('.maxapparentpower').html(maximumAcApparentPower.replace(/,(\s+)?$/, ''));
                $("input[name^='MaximumActivePower']").each(function () {
                    if (maximumActivePower != '')
                        maximumActivePower += $(this).val() + ' @Translate.Text("kW"), ';
                    else
                        maximumActivePower = $(this).val() + ' @Translate.Text("kW"), ';
                });
                jQuery('.maxactivepower').html(maximumActivePower.replace(/,(\s+)?$/, ''));
                if ('@Model.Status' == 'ReviewerRejected' || '@Model.Status' == 'Approved') {
                    var category = '';
                    var i = 0;
                    if (jQuery('#form-field-Document621091').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block' || jQuery('#form-field-Document1741').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block') {
                        category = '@Translate.Text("Residential")';
                        i++;
                    }
                    if (jQuery('#form-field-Document6100032').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block' || jQuery('#form-field-Document61000312').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block') {
                        if (i == 1) {
                            category += ', ' + '@Translate.Text("Commercial")';
                        }
                        else {
                            category = '@Translate.Text("Commercial")';
                        }
                        i++;
                    }
                    if (((jQuery('#form-field-Document6100061').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block' || jQuery('#form-field-Document6100062').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block') &&
                        jQuery('#form-field-Document6100063').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block') || ((jQuery('#form-field-Document6100063').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block' ||
                            jQuery('#form-field-Document6100064').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block') && jQuery('#form-field-Document6100062').closest('.form-field').find('[data-uploader-filename]').css('display') == 'block')) {
                        if (i == 2) {
                            category += ' @Translate.Text("and") @Translate.Text("Industrial Environment")';
                        }
                        else {
                            category = '@Translate.Text("Industrial Environment")';
                        }
                        i++;
                    }
                    if (i == 3) {
                        category = '@Translate.Text("All files uploaded")';
                    }

                    if (category == "") {
                        jQuery('.category').html('@Model.Functionofstring');
                        jQuery('#UsageCategory').val('@Model.Functionofstring');
                    }
                    else {
                         jQuery('.category').html(category);
                        jQuery('#UsageCategory').val(category);
                    }
                }
                else {
                    var datacheck = [{ "logic": "$('#form-field-Document6100061').val().length != 0 && $('#form-field-Document6100063').val().length != 0", "category": "Residential, Commercial, and Light Industrial Environment" }, { "logic": "$('#form-field-Document6100062').val().length != 0 && $('#form-field-Document6100064').val().length != 0", "category": "Industrial Environment" }, { "logic": "$('#form-field-Document6100062').val().length != 0 && $('#form-field-Document6100063').val().length != 0", "category": "All- Residential, Commercial and, Industrial Environment" }];
                    $(datacheck).each(function () {
                        var logic = this.logic;
                        category = this.category;
                        if (Function('"use strict";return (' + logic + ')')()) { jQuery('.category').html(category); jQuery('#UsageCategory').val(category); } else { console.log(category + ' is not satisfied'); };
                    });
                }

            }
            jQuery('html,body').animate({ scrollTop: 0 }, 1000);
            //window.initComponents('rs_area');
        }
        jQuery('.backbutton').click(function (e) {
            e.preventDefault();
            var pagenumber = parseInt(jQuery(this).closest('.pageclass').attr("data-page"));
            jQuery('.pageclass[data-page=' + (pagenumber - 1) + ']').removeClass('hidden');
            jQuery('.pageclass[data-page=' + (pagenumber) + ']').addClass('hidden');
            jQuery('.pageclass').not(jQuery('.pageclass[data-page=' + (pagenumber - 1) + ']')).addClass('hidden');
            var txt = jQuery('.pageclass[data-page=' + (pagenumber - 1) + ']').find('.legend').text();
            steptracker(pagenumber, txt);
            jQuery('html,body').animate({ scrollTop: 0 }, 1000);
        });
        modelName.on('focusout', function () {
            if (modelName.val() != '') {
                checkDuplicateModelName("@FileEntity.Invertermodule");
            }
        });
        function checkDuplicateModelName(equipment_type) {
            var form = jQuery("#drrginverterform");
            var token = $('input[name="__RequestVerificationToken"]', form).val();
            var url = "/api/sitecore/DRRGDashboard/CheckDuplicateApplication";

            jQuery.ajax({
                url: url,
                traditional: true,
                data: {
                    __RequestVerificationToken: token,
                    equipmentType: equipment_type,
                    modelname: modelName.val()
                },
                beforeSend: function () {
                    jQuery('.j105-drrg--loader').show();
                    jQuery('.j105-drrg--loader').css('top', $(window).scrollTop());
                    $('body').removeClass('unscrollable').addClass('unscrollable');
                    $submitBtn.attr('disabled', true);
                },
                complete: function () {
                    jQuery('.j105-drrg--loader').hide();
                    $('body').removeClass('unscrollable');
                },
                dataType: 'json',
                type: 'POST',
                success: function (response) {
                    if (response != null) {
                        if (response.errorCode != "000") {
                            valid = false;
                            errModelname.html('');
                            errModelname.html('<ul class="parsley-errors-list filled"><li class="parsley-custom-error-message">' + response.Message + '</li></ul>');
                            jQuery("#form-field-ModelName").removeClass("parsley-success");
                            jQuery("#form-field-ModelName").addClass("form-field__input--error parsley-error");
                            jQuery("#form-field-ModelName").parent('span').removeClass("form-field__input-wrapper--validated");
                            jQuery("#form-field-ModelName").parent('span').addClass("form-field__input-wrapper--error");

                        }
                        else {
                            errModelname.html('');
                            valid = true;
                            $submitBtn.attr('disabled', false);
                        }
                    }
                }
            });
        }
    });
</script>