        </div>
    </div>
</div>
<div class="j05-drrg-manager" data-journey="j05-drrg-manager">
    <div class="grid">
        @Html.Sitecore().Placeholder("pagetitle")
        <div class="grid__row mb24" style="display:none">
            <div class="grid__column grid__column--12">
                <form id="form-evaluator-filter" action="#" method="POST" form-skipvalidation="true">
                    @Html.AntiForgeryToken()
